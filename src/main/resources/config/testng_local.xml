<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web Demo Suite" verbose="0" parallel="tests"
	thread-count="50">
	<listeners>
		<listener class-name="com.quantum.listeners.QuantumReportiumListener" />
		<listener class-name="com.quantum.listeners.FailedTestSuiteListener" />
	</listeners>

	<test name="Web Scenarios Chrome Test" enabled="true" parallel="methods" thread-count="1">
		<parameter name="chrome.capabilities.browserName"
			value="chrome"></parameter>
		<parameter name="env.resources" value="src/main/resources/common" />

		<groups>
			<run>
				<include name="@web1" />
			</run>
		</groups>
		<classes>
			<class
				name="com.qmetry.qaf.automation.step.client.gherkin.GherkinScenarioFactory" />
		</classes>
	</test>
	
</suite>
